   "source": [
    "def poisson(tmax,lam):\n",
    "\n",
    "    L=[]\n",
    "    t=0\n",
    "\n",
    "    while True:\n",
    "        t+=np.random.exponential(lam)\n",
    "        if t>=tmax:\n",
    "            break\n",
    "        L.append(t)\n",
    "        \n",
    "    return np.array(L)"
   ]
  },
